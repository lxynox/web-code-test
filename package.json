{
  "name": "denver_codetest_web_frontend",
  "version": "1.0.0",
  "description": "Code test for web --front end",
  "main": "server/index.js",
  "scripts": {
    "lint": "xo",
    "dev": "webpack-dev-server --open --hot",
    "build": "NODE_ENV=production webpack --progress --hide-modules",
    "start": "node server/build"
  },
  "xo": {
    "extends": "xo-react",
    "esnext": true,
    "space": true,
    "semicolon": false,
    "env": [
      "browser",
      "node"
    ],
    "rules": {
      "new-cap": 0,
      "react/prop-types": 0,
      "react/jsx-no-bind": 0,
      "react/no-danger": 0
    },
    "overrides": [],
    "ignore": [
      "server/**",
      "client/build/**"
    ]
  },
  "babel": {
    "presets": [
      "@babel/env",
      "@babel/react"
    ]
  },
  "author": "Xuanyu",
  "license": "ISC",
  "dependencies": {
    "express": "^4.13.3",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-router-dom": "^4.1.1",
    "underscore": "^1.12.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "eslint-config-xo-react": "^0.11.1",
    "eslint-plugin-react": "^7.0.0",
    "webpack": "^2.5.0",
    "webpack-dev-server": "^2.4.5",
    "xo": "^0.18.2"
  }
}
